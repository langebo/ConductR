<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>10</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>ConductR.Autofac</PackageId>
    <Description>Autofac adapter for the ConductR package</Description>
    <Authors>Boris Lange</Authors>
    <Copyright>Copyright (c) 2022 Boris Lange</Copyright>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/langebo/ConductR</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="6.3.0" />
    <PackageReference Include="ConductR" Version="1.0.0" />
  </ItemGroup>

</Project>
